{
    "package": {
        "name": "Talipot",
        "repo": "talipot_binaries",
        "subject": "anlambert",
        "desc": "Large graphs analysis, drawing and visualization framework",
        "website_url": "http://github.com/anlambert/talipot",
        "issue_tracker_url": "https://github.com/anlambert/talipot/issues",
        "vcs_url": "https://github.com/anlambert/talipot/talipot.git",
        "github_use_tag_release_notes": true,
        "licenses": ["GPL-3.0"],
        "labels": ["data analysis", "graph", "graph drawing", "data visualization"],
        "public_download_numbers": true,
        "public_stats": true
    },

    "version": {
        "name": "@TalipotVersion@",
        "gpgSign": false
    },

    "files": [
        {
            "includePattern": "build/(Talipot.*\\.AppImage)",
            "uploadPattern": "$1",
            "matrixParams": {
                "override": 1
            }
        },
        {
            "includePattern": "build/(Talipot.*\\.dmg)",
            "uploadPattern": "$1",
            "matrixParams": {
                "override": 1
            }
        }
    ],

    "publish": true
}
